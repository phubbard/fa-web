<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.phfactor.fa-web</string>

    <key>ProgramArguments</key>
    <array>
        <string>/Users/pfh/code/fa-web/.build/release/App</string>
    </array>

    <key>WorkingDirectory</key>
    <string>/Users/pfh/code/fa-web</string>

    <key>RunAtLoad</key>
